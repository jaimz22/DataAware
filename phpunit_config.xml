<?xml version="1.0" encoding="UTF-8"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/11.1/phpunit.xsd" bootstrap="vendor/autoload.php" colors="true" cacheDirectory=".phpunit.cache">
  <coverage>
    <report>
      <clover outputFile="logs/clover.xml"/>
      <html outputDirectory="logs/coverage" lowUpperBound="35" highLowerBound="70"/>
      <text outputFile="logs/coverage.txt" showUncoveredFiles="true"/>
    </report>
  </coverage>
  <testsuites>
    <testsuite name="My Test Suite">
      <file>./tests/DataAwareTest.php</file>
    </testsuite>
  </testsuites>
  <logging/>
  <source>
    <include>
      <directory suffix=".php">./src</directory>
    </include>
  </source>
</phpunit>
